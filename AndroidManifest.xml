<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.cmoaciopm.demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="ApiDemosApplication">

        <activity
            android:name=".ApiDemos"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.ConfigChangeActivity"
            android:label="@string/activity_config_change">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="net.cmoaciopm.demo.category.ANMIN_SAMPLE_CODE" />
            </intent-filter>
        </activity>

        <activity android:name=".util.LruCacheActivity"
            android:label="@string/activity_lru_cache">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="net.cmoaciopm.demo.category.ANMIN_SAMPLE_CODE" />
            </intent-filter>
        </activity>
        
		<activity android:name=".resources.ScreenConfigActivity"
            android:label="@string/activity_screen_config">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="net.cmoaciopm.demo.category.ANMIN_SAMPLE_CODE" />
            </intent-filter>
        </activity>
        
		<activity android:name=".view.SlidingDrawerActivity"
            android:label="@string/activity_sliding_drawer">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="net.cmoaciopm.demo.category.ANMIN_SAMPLE_CODE" />
            </intent-filter>
        </activity>
        
    </application>

</manifest>